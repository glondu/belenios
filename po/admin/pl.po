#: src/web/common/mails_admin.ml:47
msgid "1. Click on the link."
msgstr "1. Kliknij link."

#: src/web/server/common/pages_admin.ml:1183
msgid "1a: action needed from trustee: generate private key"
msgstr "1a. poczekaj aż mąż (osoba) zaufania utworzy klucz prywatny"

